<h1 class="buddybuddy-logo"><img src="/images/logo.png" /></h1>

<h2>Who are you?</h2>
{{view Ember.Select content=arrangedContent 
                    value=selectedUser
                    optionLabelPath="content.name"
                    prompt="Select your name"
                    class="form-control"}}
  
<a {{action "login" selectedUser}} class="login btn">Login</a>