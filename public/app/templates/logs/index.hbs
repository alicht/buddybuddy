{{outlet}}

<h3>All Logs: </h3>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>user</th>
      <th>pairing</th>
      <th>created at</th>
      <th>message</th>
    </tr>
  </thead>

  <tbody>
    {{#each content}}
    <tr>
      <td>{{id}}</td>
      <td>[{{user.id}}] {{user.name}}</td>
      <td>[{{pairing.id}}] {{format-date pairing.startDate "MMM DD, YYYY"}} - {{format-date pairing.endDate "MMM DD, YYYY"}}</td>
      <td>{{format-date createdAt "MMM DD, YYYY"}}</td>
      <td>{{message}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

